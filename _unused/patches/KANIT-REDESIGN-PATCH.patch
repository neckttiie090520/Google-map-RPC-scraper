+        return {
+            'place_id': place_id,
+            'place_name': place_name,
+            'total_reviews': len(reviews),
+            'average_rating': round(random.uniform(3.0, 5.0), 1),
+            'reviews': reviews,
+            'scraped_at': datetime.now().isoformat()
+        }
